<script type="text/javascript">

P.when('DynamicIframe').execute(function(DynamicIframe){
	var BookDescriptionIframe = null,
	    bookDescEncodedData = "%3Ch2%3EForex%20For%20Beginners%3C%2Fh2%3E%3Cp%3EForex%20For%20Beginners%20is%20the%20prequel%20to%20my%20first%20two%20books%2C%20%3Cstrong%3EA%20Three%20Dimensional%20Approach%20to%20Forex%20Trading%3C%2Fstrong%3E%2C%20and%20%3Cstrong%3EA%20Complete%20Guide%20to%20Volume%20Price%20Analysis%3C%2Fstrong%3E.%20It%20is%20your%20primer%20to%20the%20world%20of%20forex.%20It%20has%20been%20written%20to%20lay%20the%20foundations%20and%20provide%20the%20framework%20for%20getting%20started%20in%20the%20world%20of%20forex%2C%20in%20what%20I%20believe%20is%20the%20correct%20way.%20My%20other%20books%20then%20build%20on%20what%20you%20will%20learn%20here%2C%20to%20further%20develop%20your%20trading%20skills%20and%20knowledge.%3C%2Fp%3E%3Ch2%3EApplying%20what%20you%20learn%3C%2Fh2%3E%3Cp%3EWhat%20I%20try%20to%20do%20in%20all%20my%20books%2C%20is%20to%20show%20you%20how%20to%20apply%20that%20knowledge%20to%20help%20you%20become%20a%20more%20confident%20trader.%20After%20all%2C%20learning%20is%20all%20well%20and%20good%2C%20but%20if%20we%20are%20not%20taught%20how%20to%20apply%20that%20knowledge%20in%20a%20practical%20way%2C%20then%20it%20is%20of%20little%20use.%20It%20is%20the%20application%20of%20knowledge%20that%20empowers%2C%20and%20this%20is%20what%20I%20have%20tried%20to%20do%20here%2C%20and%20in%20my%20online%20training%20rooms%2C%20and%20all%20my%20books.%3C%2Fp%3E%3Ch2%3EIt%27s%20all%20about%20risk%3C%2Fh2%3E%3Cp%3EThere%20are%20of%20course%20many%26%23xa0%3Bbooks%20about%20forex%20trading.%20What%20is%20different%20about%20this%20book%2C%20is%20the%20focus%20on%20those%20aspects%20of%20trading%20which%20I%20believe%20are%20fundamental.%20After%20all%2C%20there%20are%20only%20two%20questions%20we%20need%20to%20answer%20when%20considering%20a%20position%20in%20the%20market%3A-%3C%2Fp%3E%3Cul%3E%3Cli%3E%3Cem%3E%3Cstrong%3EWhat%20is%20the%20risk%20on%20this%20trade%20-%20high%2C%20medium%20or%20low%3F%3C%2Fstrong%3E%3C%2Fem%3E%3C%2Fli%3E%3Cli%3E%3Cem%3E%3Cstrong%3EWhat%20is%20the%20financial%20risk%20on%20this%20trade%3F%3C%2Fstrong%3E%3C%2Fem%3E%3C%2Fli%3E%3C%2Ful%3EThe%20first%20is%20the%20hardest%20question%20to%20answer%2C%20and%20the%20book%20will%20explain%20in%20detail%20the%20analysis%20and%20approach%20to%20use%2C%20in%20order%20to%20answer%20this%20question%20with%20confidence.%26%23xa0%3BThe%20second%20question%20is%20more%20straightforward%20and%20is%20answered%20provided%20you%20have%20an%20understanding%20of%20risk%2C%20money%20management%20and%20position%20sizing%20in%20relation%20to%20your%20trading%20capital.%20Again%2C%20this%20is%20covered%20in%20detail%20in%20the%20book.%26%23xa0%3BAs%20the%20tag%20line%20on%20the%20front%20cover%20says%20%3Cstrong%3E%27What%20you%20need%20to%20know%20to%20get%20started%2C%20and%20everything%20in%20between%27%3C%2Fstrong%3E%20which%20really%20sums%20up%20what%20you%20will%20learn.%3Cp%3E%3C%2Fp%3E%3Ch2%3EThis%20book%20is%20for%20you%3C%2Fh2%3E%3Cp%3EThe%20book%20explains%20everything%2C%20from%20the%20pure%20mechanics%20to%20the%20trading%20methodology%20that%20I%20advocate%2C%20and%20which%20I%20have%20used%20in%20all%20my%20own%20trading%20and%20investing%20for%20over%2017%20years.%20Forex%20For%20Beginners%20is%20also%20dedicated%20to%20all%20those%20traders%20who%20have%20asked%20me%20to%20write%20such%20an%20introduction%2C%20based%20on%20my%20knowledge%20and%20my%20methodology.%20%3Cstrong%3EThis%20book%20is%20for%20you.%3C%2Fstrong%3E%3C%2Fp%3E",
	    bookDescriptionAvailableHeight,
	    minBookDescriptionInitialHeight = 112,
	    options = {},
	    iframeId = "bookDesc_iframe";

    function resizeCallback() {
        if (true) {
            // Set the height to the number of lines specified
            var numLines = parseInt(5, 10);

            // Get the line-height of the iframe
            var iframe = document.getElementById(iframeId);
            var iframeDocument = iframe.contentDocument;
            var iframeContent = iframeDocument.getElementById("iframeContent");

            // Compute the line height
            var lineHeight = iframeDocument.defaultView.getComputedStyle(iframeContent, null).getPropertyValue("line-height");

            // Parse the line height
            lineHeight = parseFloat(lineHeight);
            bookDescriptionAvailableHeight = Math.round(lineHeight * numLines);
        } else {
            var bdOffsetTop = document.getElementById("bookDescription_feature_div").offsetTop;
            var imageBlockOffsetTop = document.getElementById("booksImageBlock_feature_div").offsetTop;
            var imageBlockHeight = document.getElementById("booksImageBlock_feature_div").offsetHeight;
            bookDescriptionAvailableHeight = imageBlockOffsetTop + imageBlockHeight -bdOffsetTop - 30;
            if(bookDescriptionAvailableHeight < minBookDescriptionInitialHeight) {
                bookDescriptionAvailableHeight = minBookDescriptionInitialHeight;
            }
        }
    		
    	var psTotalHeight = document.getElementById("postBodyPS").offsetHeight;

    	if(psTotalHeight > bookDescriptionAvailableHeight + 30){
    		if(document.getElementById("bdSeeLessPrompt").style.display == "none"){
    	    	document.getElementById("outer_postBodyPS").style.height = bookDescriptionAvailableHeight + 'px';
    	    	document.getElementById("psPlaceHolder").style.display ="block";
    	    	document.getElementById("bdSeeAllPrompt").style.display ="block";
    		}
    		else{
    			document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
    		}
        }
        else{
        	document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
        	document.getElementById("psPlaceHolder").style.display ="none";
        	document.getElementById("bdSeeAllPrompt").style.display ="block";
	    	document.getElementById("bdSeeLessPrompt").style.display ="none";
	    	document.getElementById("bdExpanderIcon").className = document.getElementById("bdExpanderIcon").className.replace("rotate","");
        }
    }
	    
	options.iframeId = iframeId;
	options.iframeWrapperId = "bookDesc_iframe_wrapper";
	options.overriddenCSSId = "bookDesc_override_CSS";
	options.encodedIframeContent = bookDescEncodedData;
	options.initialResizeCallback = resizeCallback;
	
	BookDescriptionIframe = new DynamicIframe(options);
	BookDescriptionIframe.createIframe();
	    
	if ((typeof BookDescriptionIframe != 'undefined') && (BookDescriptionIframe instanceof DynamicIframe)) {
	  P.when('jQuery').execute(function($) {
	    $(window).resize(function() {
	        BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	    $(window).bind('imageResize', function() {
	    	BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	  });
	}
});
</script>

        
        
        
        
        
        
    
    
    

    















    </div>






        
    
        
        

        









