    <div id="kcpApp_feature_div" class="feature" data-feature-name="kcpApp">




    

    
    
    
        
            




<script type="text/javascript">
P.when('A', 'jQuery').execute(function (A, $) {
    var kcpAppJS = function () {
         // see w?ShiftjisJavascript
         var chBACKSLASH = String.fromCharCode(0x5c),
             $errorText = $('#udpKcpAppDeliveryError'),
             $successText = $('#udpKcpAppDeliverySuccess'),
             $processingText = $('#udpKcpAppDeliveryInProgress'),
             StringUtil = {
                invalidInputMessage : "Invalid information. Please provide a valid email or mobile number.",
                emailMessage: "Please check your email.",
                smsMessage: "Please check your mobile phone.",
                successMessage: "The app is on the way!",
                genericErrorMessage: "Unable to process your request right now. Please try after some time."
             };

         //Registering a reftag whenever the kcpApp is loaded
         A.ajax('/gp/digital/fiona/ajax/reftagCall.html/ref=kcpapp_udp_load_embed_atf', {
                method: 'get',
                cache: false,
         });

         function isValidEmail(input) {
                var
                    emailAddress = input.val(),
                    noEmailSupport = input.data('no-email-support'),
                    pattern = new RegExp(chBACKSLASH + "w+@[" + chBACKSLASH +"w" + chBACKSLASH + "-+_]+?" + chBACKSLASH + ".[" + chBACKSLASH + "w" + chBACKSLASH + "-+_]+");

                //If email not supported or invalid email address return false
                if (noEmailSupport || (emailAddress && typeof emailAddress !== "string")) {
                    return false;
                }

                // validating email address pattern
                return pattern.test(emailAddress);
         }

         function isValidPhone(input) {
                var
                    phoneNumber = input.val(),
                    noSMSSupport = input.data('no-sms-support'),
                    pattern = new RegExp("^([0-9" + chBACKSLASH + "(" + chBACKSLASH + ")" + chBACKSLASH + "/" + chBACKSLASH + "+" + chBACKSLASH + "-]*)$"),
                    digitsOnly;

                //If sms not supported or no input return false
                if (noSMSSupport || !phoneNumber) {
                    return false;
                }

                //Validating phoneNumber pattern
                digitsOnly = phoneNumber.replace(/D/g, '');
                return (digitsOnly.length >= 9 && digitsOnly.length < 20) && pattern.test(phoneNumber);
         }

         function appendCountryCode(input) {
                var recipient = input.val(),
                    countryCode = input.data('sms-country-code');

                if (countryCode && isValidPhone(recipient) && !recipient.match(/^\+/g)) {
                     recipient = countryCode + recipient;
                }
                return recipient;
         }


         $("#udpKcpAppSendButton").bind('click', function () {
                var
                    $recipient = $('#udpKcpAppRecipient'),
                    inputData = $("#udpKcpAppForm").serializeArray(),
                    recipient = appendCountryCode($recipient),
                    communicationType;

                //Hide Previous messages if any
                $errorText.hide();
                $successText.hide();
                $processingText.hide();

                //Adding recipient the params of the AJAX request
                inputData.push({name: 'recipient',value:recipient},{name: 'toJSON',value: "1"});

                //Validate Input Data and call the component to send the email to the recipient
                if(isValidEmail($recipient)){
                    communicationType = "email";
                    inputData.push({name: 'communicationType',value: communicationType});
                    submitRTERequest(inputData,communicationType);
                } else if(isValidPhone($recipient)) {
                    communicationType = "sms";
                    inputData.push({name: 'communicationType',value: communicationType});
                    submitRTERequest(inputData,communicationType);
                } else {
                    $errorText.html(StringUtil.invalidInputMessage);
                    $errorText.show();
                }
         });

        function submitRTERequest(input,communicationType){

                /* Using the existing perl mason end point and logic
                    TODO: Move to A Java Based Email Client, issue: DBSMIDAS-822
                */

                //Disable the send button and show processing text
                $('#udpKcpAppSendButton').prop("disabled", true);
                $processingText.show();

                //Sending AJAX request
                A.ajax('/gp/digital/fiona/ajax/send-email-or-sms.html/ref=kcpapp_udp_snt_embed_atf', {
                    method: 'post',
                    params: input,
                    success: function (data) {
                        $processingText.hide();
                        // If the server sends a string convert it into JSON
                        if(typeof(data) == "string"){
                            data=JSON.parse(data);
                        }

                        // show success message if the status returned is 1
                        if(data && data.status) {
                            $successText.html(StringUtil.successMessage + " " + ((communicationType == 'email') ? StringUtil.emailMessage : StringUtil.smsMessage));
                            $successText.show();
                        } else {
                            data['message'] = (data.status) ? "Invalid Request" : data.message;
                            $errorText.html((data.message) ? data.message : StringUtil.genericErrorMessage);
                            $errorText.show();
                        }

                        $('#udpKcpAppSendButton').prop("disabled", false);
                    },
                    error: function () {
                        $processingText.hide();
                        $errorText.html(StringUtil.genericErrorMessage);
                        $errorText.show();
                        $('#udpKcpAppSendButton').prop("disabled", false);
                    }
                });

                //Clear the input data
                $('#udpKcpAppRecipient').val("");
        }
    }
    return new kcpAppJS();
});
</script>


    <div id="udpKcpApp" class="a-fixed-left-grid"><div class="a-fixed-left-grid-inner" style="padding-left:50px">
        <div class="a-fixed-left-grid-col a-col-left" style="width:50px;margin-left:-50px;_margin-left:-25px;float:left;">
            <img alt="" src="https://images-na.ssl-images-amazon.com/images/G/01/x-locale/common/transparent-pixel._V192234675_.gif" class="a-thumbnail-left" id="udpKcpAppImage">
        </div>
        <div class="a-fixed-left-grid-col a-col-right" style="padding-left:0%;*width:99.6%;float:left;">
            <p class="a-spacing-none a-color-base a-text-bold">
                Get the Free Kindle App
            </p>
            <p class="a-spacing-micro a-spacing-top-micro a-size-small a-color-base">
                Enter email or phone number to get a link
            </p>
        </div>
    </div></div>
    <div class="a-section a-spacing-mini a-spacing-top-mini">
        <form id="udpKcpAppForm" method="post" action="javascript:void(0);">
            <input type="hidden" name="downloadLink" value="http://www.amazon.com/gp/kindle/kcpApp.html">
            
            <div class="a-section a-spacing-none a-spacing-top-micro a-padding-none">
                <input type="text" id="udpKcpAppRecipient" placeholder="Email or mobile number" class="a-input-text" data-no-email-support="false" data-no-sms-support="false" data-sms-country-code="">
                <span class="a-button a-button-small"><span class="a-button-inner"><input id="udpKcpAppSendButton" class="a-button-input" type="submit"><span class="a-button-text" aria-hidden="true">Send</span></span></span>
            </div>
        </form>
        <p id="udpKcpAppMessages">
                <span id="udpKcpAppDeliveryInProgress">
                    Processing your request...
                    <img alt="" src="http://g-ecx.images-amazon.com/images/G/01/kindle/www/myk/icon_spinner._CB192549371_.gif" id="spinnerImage">
                </span>
                <span id="udpKcpAppDeliverySuccess" class="a-size-small a-color-success"></span>
                <span id="udpKcpAppDeliveryError" class="a-size-small a-color-error"></span>
        </p>
    </div>
    




<style type="text/css">
#udpKcpAppRecipient {
    height: 22px;
    font-size: 12px;
    width: 150px;
    margin-left: 0px;
}

