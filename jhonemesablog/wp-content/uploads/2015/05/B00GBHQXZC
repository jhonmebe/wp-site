    #sendSampleBox {
        border-radius: 4px;
        box-shadow: 0 0 0 4px rgba(0,0,0,0.1);
    }

    #sendSampleButton {
        width : 100%;
    }

    #sendSampleDeliverTo .a-button-dropdown {
        width : 100%;
    }
